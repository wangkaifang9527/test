  align-items: center;
  justify-content: space-between; 
}
.list_right_san .car_edit{
  width:100rpx;
  background:#13344B;
  color:#fff;
  font-size:24rpx;
  text-align:center;
  height:38rpx;
  padding:5rpx;
  border-radius:10rpx;

  
}
.list_right_san .li{
  width:100rpx;
  height:38rpx;
  line-height: 38rpx;
  background:rgba(19,52,75,1);
