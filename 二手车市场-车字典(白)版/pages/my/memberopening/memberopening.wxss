/*充值*/
.input{
  background: #E6E6E6;
  border-radius: 10rpx;
  margin-top: 20rpx;
  padding:15rpx;
  width: 670rpx;
}
.list_jinqian{
  overflow: hidden;
  width: 700rpx;
}
.list_jinqian view{
  float: left;
  width: 23%;
  background: #E6E6E6;
  margin-right: 2.66%;
  margin-top: 20rpx;
  text-align: center;
  padding: 20rpx 0rpx;
  border-radius: 10rpx;
  box-shadow:1px 0px 10px rgba(0,0,0,0.1);
}
