.container{
  padding: 40rpx 40rpx;
  color: #fff;
}
.kf_title{
  font-size: 40rpx;
  text-align: center;
  padding-bottom: 40rpx;
  letter-spacing: 4rpx;
}
.kf_image image{
  width: 100%;
  height: 300rpx;
}
.kf_type{
  display: flex;
  flex-direction: row;
  justify-content:  space-between;
  border-bottom: 1rpx solid rgba(255, 255, 255, 0.3);
  padding: 30rpx 0rpx;
  letter-spacing: 2rpx;
}
.kf_type .kf_tishiyu{
  font-size: 20rpx;
  color: rgba(255, 255, 255, 0.3);

}
.kf_type .kf_button{
  background: rgba(255, 255, 255, 0.3);
  padding: 10rpx 20rpx;
}