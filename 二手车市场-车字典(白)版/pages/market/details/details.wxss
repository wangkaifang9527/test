/* 规则 */
.bmgz{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: baseline; 
  margin: 0 auto;
  width: 220rpx;
  position: relative;
}
.bmgz image{
  height: 30rpx;
  width: 55rpx;
  position: relative;
}
.B{
  animation: b 2s infinite
}
@keyframes b{
  0%{transform: rotate(-10deg);}
  50%{transform: rotate(10deg);}
  100%{transform: rotate(-10deg);}
}
.M{
  animation: m 2s infinite
}
@keyframes m{
  0%{transform: rotate(10deg);}
  50%{transform: rotate(-10deg);}
  100%{transform: rotate(10deg);}
}
.G{
  animation: g 2s infinite
}
@keyframes g{
  0%{transform: rotate(-10deg);}
  50%{transform: rotate(10deg);}
  100%{transform: rotate(-10deg);}
}
.Z{
  animation: z 2s infinite
}
@keyframes z{
  0%{transform: rotate(10deg);}
  50%{transform: rotate(-10deg);}
  100%{transform: rotate(10deg);}
}
