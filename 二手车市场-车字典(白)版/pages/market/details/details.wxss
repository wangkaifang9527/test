/* 担保订车 新字 */
.new{
  width: 0rpx;
  height:110rpx;
  position:relative;
  left:66%;
  margin-top:-110rpx;
  animation: new 1s infinite
}
@keyframes new{
  0%{transform: scale(.6,.6);}
  20%{transform: rotate(0deg)}
  80%{transform: rotate(-10deg)}
  100%{transform: scale(1,1);}
}

