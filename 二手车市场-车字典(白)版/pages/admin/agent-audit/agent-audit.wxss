page {
    background: #f8f8f8;
}

.container {
}

.withdraw_audit_box {
    padding: 0 30rpx 15px 30rpx;
}

.withdraw_audit_li {
    width: 100%;
    background: #fff;
    margin-top: 15px;
}

.withdraw_audit_li .info {
    padding: 0 30rpx;
    color: #333;
    height: 187rpx;
    border-bottom: 1px solid #e6e6e6;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}

.withdraw_audit_li .info .intr {
    flex: 1;
}

.withdraw_audit_li .info .intr .p {
    font-size: 21px;
}

.withdraw_audit_li .info .intr .p text {
    font-size: 16px;
    margin-left: 20rpx;
}

.withdraw_audit_li .info .intr .h1 {
    font-size: 16px;
    margin-top: 5px;
}

.withdraw_audit_li .btn {
    border: 1px solid #1c1e2f;
    border-radius: 5px;
    background: #fff;
    width: 118rpx;
    height: 54rpx;
    line-height: 54rpx;
    text-align: center;
}

.withdraw_audit_li .btn.gary {
    border: 1px solid #999;
    color: #999;
}

.withdraw_audit_li .time {
    height: 68rpx;
    line-height: 68rpx;
    color: #696b7b;
    padding-left: 30rpx;
}
