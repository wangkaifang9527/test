
.container .title {
    font-size: 17px;
    margin: 15px 0;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between; 
    width: 710rpx;
}
.container .title .report{
  background: #14AC67;
  color: #fff;
  border-radius: 5rpx;
  height: 65rpx;
  width: 165rpx;
  text-align: center;
  line-height: 65rpx;
}
.container .title .right{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between; 
  width: 40%;
}
.container .title .right image{
  width: 77rpx;
  height: 85rpx;
}
.container .title text {
    font-size: 20rpx;
    margin-left: 5px;
    border-radius: 100px;
    background: #999;
    color: #fff;
    min-width: 10px;height: 18px;line-height: 18px;
    padding:0 4px;
    text-align: center
}

.box {
    margin-bottom: 15px;
    background: #fff;
    border-radius: 10px;
    padding: 0 30rpx;
    overflow: hidden;
}

/* 车辆概要 */

.car_detail {
    padding-bottom: 15px;
    width: 650rpx;
}

.car_detail .line {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    margin-bottom: 15px;
}

.car_detail .line text:nth-child(1) {
    color: #666;
}

.car_detail .line text:nth-child(2) {
    color: #333;
}

.car_detail .line.top {
    padding: 20px 0;
    border-bottom: 1px solid #dedede;
}

.car_detail .line.top text:nth-child(1) {
    font-size: 15px;
}

/* 出险报告 */

.result_detail {
    background: transparent;
    padding: 0;
    width: 710rpx;
}

.result_detail .line {
    margin-bottom: 15px;
    background: #fff;
    border-radius: 10px;
    position: relative;
    padding: 15px;
    /* background: url("http://www.levau.com/banner/shuiyin.png") no-repeat;    
    background-size:  100% 100%;  */
}

.result_detail .line image {
    position: absolute;
    width: 8px;
    height: 37px;
    top: -23px;
}

.result_detail .line:first-child image {
    display: none;
}

.result_detail .line .left_img {
    left: 15px;
}

.result_detail .line .right_img {
    right: 15px;
}

.result_detail .line .top {
}

.result_detail .line .top .h1 {
    font-size: 16px;
    margin-bottom: 15px;
    display: block;
}

.result_detail .line .top .top_li {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 15px;
}

.result_detail .line .top .top_li .left {
    color: #666;
}

.result_detail .line .top .top_li .right {
}

.result_detail .line .table {
    /* text-align: center */
}

.result_detail .line .table .tr {
    display: flex;
    flex-direction: row;
    align-items: flex-start;
    justify-content: space-between;
    padding: 15px 0;
    border-top: 1px solid #dedede;
}

.result_detail .line .table .tr.theader {
    font-size: 16px;
}

.result_detail .line .table .tr.theader text {
    color: #333 !important;
}

.result_detail .line .table .tr text {
    color: #666;
}

.result_detail .line .table .tr text:first-child {
    flex: 1;
}

.result_detail .line .table .tr text:nth-child(2) {
    width:160rpx;
    text-align: right;
    margin-left: 15px;
    font-size: 13px;
}

.result_detail .line .table .tr text:last-child {
}

/* 保单报告 */

.accidents_detail {
}

.accidents_detail .line {
    display: flex;
    flex-direction: row;
    align-items: center;
    padding: 15px 0;
    border-top: 1px solid #e6e6e6;
}

.accidents_detail .line text {
    flex: 1;
    color: #666;
    text-align: center;
}

.accidents_detail .line.top {
    font-size: 16px !important;
    text-align: center;
    padding: 15px 0;
    border: 0;
}

.accidents_detail .line.top text {
    color: #333;
}



